    <!-- http://sjka.site/ontology/seips#Environment -->


    <owl:Class rdf:about="http://sjka.site/ontology/seips#Environment">
        <rdfs:comment xml:lang="en">The physical and contextual surroundings in which tasks are performed.</rdfs:comment>
        <rdfs:label xml:lang="en">Environment</rdfs:label>
    </owl:Class>
    


    <!-- http://sjka.site/ontology/seips#Organisation -->


    <owl:Class rdf:about="http://sjka.site/ontology/seips#Organisation">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <rdfs:comment xml:lang="en">An organisational unit responsible for tasks and provision of resources in the work system (modelled as a PROV Agent).</rdfs:comment>
        <rdfs:label xml:lang="en">Organisation</rdfs:label>
    </owl:Class>
    


    <!-- http://sjka.site/ontology/seips#Person -->


    <owl:Class rdf:about="http://sjka.site/ontology/seips#Person">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <rdfs:comment xml:lang="en">A human actor in the SEIPS work system.</rdfs:comment>
        <rdfs:label xml:lang="en">Person</rdfs:label>
    </owl:Class>
    


    <!-- http://sjka.site/ontology/seips#Task -->


