    <!-- http://sjka.site/ontology/atlas#playsRole -->


    <owl:ObjectProperty rdf:about="http://sjka.site/ontology/atlas#playsRole">
        <rdfs:domain rdf:resource="http://sjka.site/ontology/atlas#Actor"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <rdfs:comment xml:lang="en">Links an Actor to a role agent (e.g., DataProducer or DataConsumer) used for accountability modelling. Organisational responsibility can then be asserted using prov:actedOnBehalfOf from the role-agent to an Organisation.</rdfs:comment>
        <rdfs:label xml:lang="en">playsRole</rdfs:label>
    </owl:ObjectProperty>
    


